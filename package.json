{
  "name": "hiaudio",
  "version": "0.0.1",
  "description": "Collaborative online DAW",
  "author": "Telecom Paris",
  "browserslist": "since 2018, not ie <= 8",
  "website": "https://hiaudio.fr",
  "scripts": {
    "prepare-dev": "rm -rf .parcel-cache public",
    "prepare-build": "rm -rf .parcel-cache ../public",
    "dev": "npm run prepare-dev && parcel --no-cache src/*.html src/static/*.html 'src/static/videos/*.mp4' --port 8000 --https --dist-dir public",
    "build": "npm run prepare-build && parcel build 'src/*.html' 'src/static/*.html' 'src/static/videos/*.mp4' --dist-dir ../public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@parcel/transformer-worklet": "^2.12.0",
    "parcel": "^2.12.0",
    "sharp": "^0.31.3"
  },
  "keywords": [
    "waveform",
    "audio",
    "audacity",
    "stem",
    "tracks",
    "multitrack",
    "playlist",
    "music",
    "editor",
    "record",
    "recording",
    "player",
    "webaudio",
    "daw",
    "multitrack",
    "metronome",
    "latency"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:idsinge/hiaudio_webapp.git"
  },
  "bugs": {
    "url": "https://github.com/idsinge/hiaudio_webapp/issues"
  },
  "homepage": "https://github.com/idsinge/hiaudio_webapp"
}
